<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net47;netstandard2.0</TargetFrameworks>
    <Title>DotNet Push Notification</Title>
    <Description>Push Notification Service Library</Description>
    <Copyright>Copyright OdinSoftware Co., Ltd. 2017</Copyright>
    <AssemblyTitle>DotNet.Push.Core</AssemblyTitle>
    <VersionPrefix>1.0.3</VersionPrefix>
    <Authors>SEONGAHN LEE</Authors>
    <AssemblyName>DotNet.Push.Core</AssemblyName>
    <PackageId>DotNet.Push.Core</PackageId>
    <PackageTags>sdk;odinsoft;push;apns</PackageTags>
    <PackageIconUrl>https://github.com/lisa3907/DotnetPush/blob/master/logo-files/dotnet.push.ico?raw=true</PackageIconUrl>
    <PackageProjectUrl>https://github.com/lisa3907/DotnetPush</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/lisa3907/DotnetPush/blob/master/LICENSE</PackageLicenseUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>DotNet.Push.Core</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>2.0.1</Version>
    <PackageReleaseNotes>upgrade .net core 2.0 &amp; net47</PackageReleaseNotes>
    <FileVersion>2.0.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="jose-jwt" Version="2.4.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="System.Net.Http.WinHttpHandler" Version="4.4.0" />
  </ItemGroup>
  
</Project>